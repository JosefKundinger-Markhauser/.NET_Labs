@{
    ViewData["Title"] = "DisplayPerson";
}

<h3>Displaying Person</h3>

<table>
    <tr>
        <td>First Name: </td>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <td>Last Name: </td>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <td>Age: </td>
        <td>@Model.Age</td>
    </tr>
    <tr>
        <td>Email Address: </td>
        <td>@Model.EmailAddress</td>
    </tr>
    <tr>
        <td>Date of Birth: </td>
        <td>@Model.DateOfBirth</td>
    </tr>
    <tr>
        <td>Description: </td>
        <td>@Model.Description</td>
    </tr>
</table>
